
<div class="{{ classes }}">
    {{ title_prefix }}
    {% if title|render %}
        {{ title }}
    {% endif %}
    {{ title_suffix }}
    {% if header|render %}
        <div class="view-header">
        {{ header }}
        </div>
    {% endif %}

    {% if exposed|render %}
        <div class="view-filters">
        {{ exposed }}
        </div>

    {% endif %}

    {% if attachment_before|render %}
        <div class="attachment attachment-before">
          {{ attachment_before }}
        </div>
    {% endif %}
    {% if rows|render %}
        <div class="view-content">
        <section class="no-top-padding">
          <div class="featured-news">
            <ul class="tabs">
              {{ rows }}

              <div class="clear"></div>
              </ul>

              {% if footer|render %}
                <div class="tabs-content">
                    {{ footer }}

                    </div>
              {% endif %}
          </div>
    	</section>


        </div>
    {% elseif empty|render %}
        <div class="view-empty">
          {{ empty }}
        </div>
    {% endif %}

    {% if pager|render %}
        <div class="item-list-bottom">
            {{ pager }}
        </div>
    {% endif %}

    {% if attachment_after|render %}
        <div class="attachment attachment-after">
          {{ attachment_after }}
        </div>
    {% endif %}

    {% if more|render %}
        {{ more }}
    {% endif %}



    {% if feed_icon|render %}
        <div class="feed-icon">
            {{ feed_icon }}
        </div>
    {% endif %}

</div>
